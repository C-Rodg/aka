{
	"manifest_version": 3,
	"name": "aka",
	"description": "Enable URL aliases for all of your favorite sites.",
	"version": "1.0",
	"author": "https://curtisrodgers.com",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": "images/bar_icon.png"
	},
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"permissions": ["tabs"]
}
